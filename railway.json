{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
        "startCommand": "cd prediction && gunicorn prediction.wsgi:application --bind 0.0.0.0:$PORT --log-level warning --access-logfile - --error-logfile -",
    "buildCommand": "cd prediction && python manage.py deploy",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
